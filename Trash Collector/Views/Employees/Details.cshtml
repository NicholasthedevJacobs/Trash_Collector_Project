<section class="scripts">
    <script type="text/javascript">
        "use strict";
        let map;

        function initMap() {
            map = new google.maps.Map(document.getElementById("map"), {
                center: {
                    lat: -34.397,
                    lng: 150.644
                },
                zoom: 8
            });
        }

    </script> 
    <iframe width="600"
